[name]
cmd

[keywords]
同步

[function]
const aify = require('aify');

module.exports = function(sCmd=''){

    if ( !sCmd ) return;

	console.debug('$> ' + sCmd);

	const execa = aify.skill('airequire').apply('execa');
    let rs = execa.shellSync(sCmd);
	if ( rs.stderr ) {
		console.error( rs.stderr );
	}else{
		console.log( rs.stdout );
	}

    return !rs.code;
};
