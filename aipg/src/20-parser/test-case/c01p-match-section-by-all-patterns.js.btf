[test]
async t => {
    let rsReader = await reader({file: './test/parser/{FILE_NAME}.xlsx'});

    let opts = {'{FILE_NAME}': async (root, context) => {
        await root.walk( NodeTypes.SheetSection, (node, object) => {
            if (object.value === 'hello world服务'){
                t.is(object.matchs[0].type, NodeTypes.Note);
            }else{
                t.is(object.matchs.length >= 2, true);
            }

        }, {readonly: true});

       writeJson('./src/20-parser/test-case/{FILE_NAME}.btf', root);
    }};

    await parser(rsReader.result, opts);

}
