[test]
async t => {
    let rsReader = await reader({file: './test/parser/{FILE_NAME}.xlsx'});

    let opts = {'{FILE_NAME}': async (root, context) => {
        let cloneRoot = root.clone();
        await cloneRoot.walk( Types.SheetSection, (node, object) => {
            if (object.cell === 'B2'){
                t.is(object.value, 'hello world服务');
            }
            if (object.cell === 'C4'){
                t.is(object.value, '返回:\t“Hello ” + 参数');
            }
        }, {readonly: true});

       //writeJson('./src/20-parser/test-case/{FILE_NAME}.btf', cloneRoot);
    }};

    await parser(rsReader.result, opts);

}
